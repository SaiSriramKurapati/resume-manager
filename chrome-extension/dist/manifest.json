{
  "manifest_version": 3,
  "name": "Resume Manager",
  "version": "1.0.0",
  "description": "Manage your resumes for job applications",
  "permissions": [
    "storage",
    "tabs"
  ],
  "action": {
    "default_title": "Resume Manager"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "externally_connectable": {
    "matches": [
      "http://localhost:3001/*"
    ]
  },
  "content_scripts": [
    {
      "js": [
        "assets/contentScript.tsx-IGfbz0k_.js"
      ],
      "matches": [
        "*://*/*jobs*",
        "*://*/*careers*",
        "*://*/*job*",
        "*://*/*career*",
        "*://jobs.ashbyhq.com/*",
        "*://boards.greenhouse.io/*",
        "*://jobs.lever.co/*"
      ]
    },
    {
      "js": [
        "assets/contentScriptMainApp.js-ddKUobp4.js"
      ],
      "matches": [
        "http://localhost:3001/*"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "matches": [
        "*://*/*",
        "*://*/*",
        "*://*/*",
        "*://*/*",
        "*://boards.greenhouse.io/*",
        "*://jobs.ashbyhq.com/*",
        "*://jobs.lever.co/*"
      ],
      "resources": [
        "assets/contentScript.tsx-IGfbz0k_.js"
      ],
      "use_dynamic_url": false
    },
    {
      "matches": [
        "http://localhost:3001/*"
      ],
      "resources": [
        "assets/contentScriptMainApp.js-ddKUobp4.js"
      ],
      "use_dynamic_url": false
    }
  ]
}
